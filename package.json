{
  "name": "master-class-react-redux-saga",
  "version": "1.0.0",
  "description": "Projet template pour la master class React/Redux/Saga",
  "main": "index.js",
  "sideEffects": [
    "**/*.css",
    "**/*.scss",
    "**/*.less"
  ],
  "scripts": {
    "start:dev": "webpack-dev-server --progress --colors --config ./webpack/webpack.dev.config.js --inline --hot",
    "start:server": "json-server --watch ./config/data/db.json",
    "build:prod": "webpack --config ./webpack/webpack.prod.config.js",
    "analyze": "webpack --config ./webpack/webpack.prod.config.js --profile --json > stats-prod.json && webpack-bundle-analyzer stats-prod.json dist/",
    "lint": "eslint --config ./.eslintrc.js --ignore-path ./.eslintignore --cache-location ./.eslintcache \"src/**/*.js\" \"src/**/*.jsx\" --cache",
    "lint:debug": "eslint --config ./.eslintrc.js --ignore-path ./.eslintignore --cache-location ./.eslintcache \"src/**/*.js\" \"src/**/*.jsx\" --cache --debug",
    "lint:fix": "eslint --config ./.eslintrc.js --ignore-path ./.eslintignore --cache-location ./.eslintcache \"src/**/*.js\" \"src/**/*.jsx\" --cache --fix",
    "test": "jest",
    "test:update": "jest --updateSnapshot",
    "test:coverage": "jest --coverage --colors"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm test"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/helabenkhalfallah/master-class-react-redux-saga.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/helabenkhalfallah/master-class-react-redux-saga/issues"
  },
  "homepage": "https://github.com/helabenkhalfallah/master-class-react-redux-saga#readme",
  "dependencies": {
    "@ant-design/icons": "=4.2.2",
    "antd": "=4.7.0",
    "axios": "=0.21.1",
    "core-js": "=3.9.1",
    "jquery": "^3.6.0",
    "prop-types": "=15.7.2",
    "react": "=17.0.0",
    "react-dom": "=17.0.0",
    "react-hot-loader": "=4.13.0",
    "react-redux": "=7.2.1",
    "react-router": "=5.2.0",
    "react-router-dom": "=5.2.0",
    "redux": "=4.0.5",
    "redux-saga": "=1.1.3",
    "regenerator-runtime": "=0.13.7",
    "url-search-params-polyfill": "=5.1.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "=7.16.3",
    "@babel/eslint-plugin": "=7.14.5",
    "@babel/parser": "=7.7.5",
    "@babel/plugin-proposal-class-properties": "=7.13.0",
    "@babel/plugin-proposal-decorators": "=7.13.15",
    "@babel/plugin-proposal-nullish-coalescing-operator": "=7.13.8",
    "@babel/plugin-proposal-optional-chaining": "=7.13.12",
    "@babel/plugin-transform-runtime": "=7.13.15",
    "@babel/preset-env": "=7.14.1",
    "@babel/preset-react": "=7.13.13",
    "babel-loader": "=8.1.0",
    "chai": "=4.2.0",
    "css-loader": "=3.4.2",
    "css-minimizer-webpack-plugin": "=3.0.1",
    "eslint": "=8.3.0",
    "eslint-config-airbnb": "=19.0.1",
    "eslint-plugin-import": "=2.25.3",
    "eslint-plugin-jsx-a11y": "=6.5.1",
    "eslint-plugin-react": "=7.27.1",
    "eslint-plugin-react-hooks": "=4.3.0",
    "html-loader": "=1.3.2",
    "html-webpack-plugin": "=4.5.1",
    "husky": "=6.0.0",
    "jest": "=27.0.4",
    "json-server": "=0.16.2",
    "less": "=3.10.3",
    "less-loader": "=5.0.0",
    "mini-css-extract-plugin": "=1.6.0",
    "optimize-css-assets-webpack-plugin": "=6.0.0",
    "sass": "=1.32.8",
    "sass-loader": "=8.0.2",
    "style-loader": "=2.0.0",
    "terser-webpack-plugin": "=4.2.3",
    "webpack": "=4.43.0",
    "webpack-bundle-analyzer": "=3.8.0",
    "webpack-cli": "=3.3.12",
    "webpack-dev-server": "=3.11.0"
  }
}
